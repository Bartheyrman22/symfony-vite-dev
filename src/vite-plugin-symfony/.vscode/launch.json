  "version": "0.2.0",
  "configurations": [
    {
      "name": "Vitest dev",
      "program": "${workspaceFolder}/node_modules/.bin/vitest",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "index"
      ],
      "type": "node",
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "development"
      },
    },
  ]
}